{
  "name": "react-dom-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tailwind:watch": "postcss src/styles/styles.css -o src/styles/output.css -w",
    "tailwind:build": "postcss src/styles/styles.css -o src/styles/output.css",
    "lint": "node_modules/.bin/tslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "coverage": "react-scripts test --coverage --runInBand",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sentry/browser": "^5.6.3",
    "@tailwindcss/custom-forms": "^0.2.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.141",
    "@types/node": "^12.7.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "classnames": "^2.2.6",
    "docz-core": "0.11.2",
    "docz-plugin-css": "0.11.0",
    "lodash": "^4.17.15",
    "postcss-import": "^12.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.2",
    "tailwindcss": "^1.1.2",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.2",
    "webpack": "4.40.2"
  },
  "devDependencies": {
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "postcss-cli": "^6.1.3",
    "react-hot-loader": "^4.12.11",
    "stylelint-config-recommended": "^2.2.0"
  },
  "publishConfig": {
